<div class="inner_content">


  <% counter = 0 %>

  <table border="1px">
    <tr>
      <td></td>
      <td></td>
      <td></td>
      <td><%=  link_to "Add Symbol" , new_symbol1_path %></td>
      <td><%= link_to t('ui.action.back'), :back ,:class => "btn btn-mini btn-info" %></td>
    </tr>
    <%= paginate @symbol1s %>
    <% @symbol1s.each do |symbol| %>
        <% if counter % 5 == 0 %>
            <% if counter > 0 %>
                </tr>
            <% end %>
            <tr>

              <td><%=  image_tag symbol.symbol_image.url(:medium) %><br/> <%= symbol.word%> </td>
              <% counter = counter + 1 %>

        <%  else %>
            <td>
              <%=  image_tag symbol.symbol_image.url(:medium) %> <br/> <%= symbol.word %>
              <% counter = counter + 1 %>
            </td>
        <% end %>
    <% end %>
  </table>

</div>