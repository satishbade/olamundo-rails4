<%= form_for(@relatedsymbol ,:html => {:multipart => true}) do |f|  %>
    <table style="float: right;margin-left:10px;" id="field_table">
      <tr>
        <td>
          <%= "Icon:" %>
        </td>
        <td>
          <%= f.file_field :symbol_image %>
        </td>
      </tr>
      <tr>
        <td>
          <%= "Text:" %>
        </td>
        <td>
          <%= f.text_field :symbol_text %>
        </td>
      </tr>
      <tr>
        <td>
          <%= f.label "Sequence:" %>
        </td>
        <td>
          <select name="select_sequence" >
            <% @sequence_select.each do |key| %>
                <% if @relatedsymbol.sequence == key %>
                    <option selected="selected"><%= key %></option>
                <% else %>
                    <option><%= key %></option>
                <% end %>
            <% end %>
          </select>
        </td>
      </tr>
        <td colspan="2">
          <div class="actions" style="margin-left: 60%;display: inline;">
            <%= f.submit t('ui.action.save'), :class => "buttonstyle_orange_small", :onclick => "spinner_load();" %>
            <%= link_to t('ui.action.cancel'), :back, :class => "buttonstyle_orange_small" %>
          </div>
        </td>
      </tr>
    </table>
<% end %>






